<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Welcome</title>
    <link rel="stylesheet" href="/css/page.css" />
  </head>
  <body>
    <div class="main-container">
      <h1 class="heading">Welcome to E-Commerce</h1>
      <div class="section">
        <h2>Already have an account?</h2>
        <button id="login" class="action-btn" onclick="goToPageLogin()">
          Login
        </button>
      </div>
      <div class="section">
        <h2>Don't have an account?</h2>
        <button id="signup" class="action-btn" onclick="goToPageSignUp()">
          SignUp
        </button>
      </div>
    </div>
    <script src="/jsExpress/loginSignup.js"></script>
  </body>
</html>
